<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>
  <appSettings>
    <!-- Manual mode -->
    <add key="ManualMode" value="Y" />
    <!-- Target test -->
    <add key="Host" value="127.0.0.1" />
    <add key="Port" value="4100" />
    <add key="AETserver" value="VeriSee" />
    <add key="AETclient" value="StressTest" />
    <!-- Test count -->
    <add key="Count" value="10000" />
    <!-- Send time interval unit ms, eg 3000 or "randon" -->
    <add key="Interval" value="random" />
    <add key="RandomIntervalmin" value="1000" />
    <add key="RandomIntervalmax" value="30000" />
    <!-- Parallel send thread -->
    <add key="Thread" value="2" />
    <!-- Test DICOM path -->
    <add key="TestDICOMPath" value="./TestDicom/test.dcm" />
  </appSettings>
</configuration>